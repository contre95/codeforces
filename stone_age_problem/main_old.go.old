package main

import (
	"bufio"
	"fmt"
	"os"
	"strconv"
	"strings"
)

const TOO_LONG = 10

func getInput(in *bufio.Reader) []int {
	numbs := readLineNumbs(in)
	arr := make([]int, len(numbs))
	for i, n := range numbs {
		val, _ := strconv.Atoi(n)
		arr[i] = val
	}
	return arr
}

